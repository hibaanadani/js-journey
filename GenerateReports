function generateReports(student){
    //first array method to use is map to iterate over the array of students and creare a new array of objects with the name, average and grade of each student
    return student.map(student => { 
        const totalScore = student.scores.reduce((sum, score) => sum + score, 0); // 2nd array method to get the total score of each student
        const average = Math.round(totalScore / student.scores.length); // 3rd array method to get the average score of each student rounded
})
}
const students = [
    { name: "Alice", scores: [90, 85, 92] },
    { name: "Bob", scores: [70, 68, 72] },
    { name: "Charlie", scores: [100, 100, 100] }
  ];
  